    "buildctl",
    "container",
    "docker",
    "build"
  ],
  "optionalDependencies": {
    "buildctl-darwin-arm64": "0.22.0",
    "buildctl-darwin-amd64": "0.22.0",
    "buildctl-linux-amd64": "0.22.0",
    "buildctl-linux-arm64": "0.22.0",
    "buildctl-linux-arm-v7": "0.22.0",
    "buildctl-linux-ppc64le": "0.22.0",
    "buildctl-linux-riscv64": "0.22.0",
    "buildctl-linux-s390x": "0.22.0",
    "buildctl-win32-amd64": "0.22.0",
    "buildctl-win32-arm64": "0.22.0"
  }
}
