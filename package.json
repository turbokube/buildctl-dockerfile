{
  "name": "buildctl",
  "version": "0.22.0-1",
  "description": "CLI for BuildKit, a toolkit for converting source code to build artifacts",
  "homepage": "https://github.com/moby/buildkit",
  "repository": {
    "type": "git",
    "url": "https://github.com/turbokube/buildctl-npm.git"
  },
  "license": "Apache-2.0",
  "bin": {
    "buildctl": "bin/buildctl-build"
  },
  "scripts": {
    "postinstall": "node bin/postinstall.js"
